{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "declaration": true,
    "baseUrl": "",
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "paths": {
      "@ngx-server-push/common": [ "../common" ]
    }
  },
  "exclude": [
    "../../node_modules"
  ]
}
